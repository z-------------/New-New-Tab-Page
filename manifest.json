{
   "chrome_url_overrides": {
      "newtab": "newtab.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://api.wunderground.com https://api.github.com; object-src 'self'",
   "description": "A minimal new tab page replacement",
   "icons": {
      "128": "icon128.png"
   },
   "author": "Zachary Guard",
   "manifest_version": 2,
   "name": "New New Tab Page",
   "options_page": "options.html",
   "version": "3.8.2",
   "short_name": "NNTP",
   "permissions": [
      "topSites",
      "management",
      "bookmarks"
   ],
   "background": {
      "page": "newtab.html"
   }
}
