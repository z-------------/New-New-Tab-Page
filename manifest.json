{
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://api.wunderground.com https://*.googleapis.com https://*.staticflickr.com; object-src 'self'",
    "description": "A minimal new tab page replacement",
    "icons": {
        "128": "img/icon128.png"
    },
    "author": "Zachary Guard",
    "manifest_version": 2,
    "name": "New New Tab Page",
    "options_page": "options/options.html",
    "version": "4.1.0",
    "short_name": "NNTP",
    "permissions": [
        "topSites",
        "management",
        "bookmarks",
        "storage"
    ],
    "background": {
        "page": "newtab.html",
        "persistent": true
    }
}