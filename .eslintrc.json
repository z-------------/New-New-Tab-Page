{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "chrome": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [ "error", 2 ],
        "quotes": [ "error", "double" ],
        "semi": [ "warn", "never" ],
        "no-undef": [ "off" ],
        "no-console": [ "off" ],
        "eqeqeq": ["error", "always"]
    }
}